// Palindrome Number checker :D



#include <stdio.h>

int main()
{
int number;
printf("please enter a number consist of 5 digits :) \n");
scanf("%d",&number);
int n1,n2,n4,n5;
n1 = (number/10000)%10;
n2 = (number/1000)%10;
n4 = (number/10)%10;
n5 = number%10;
if (n1==n5 && n2==n4)
{
    printf("it's palindrome number\n");

}
else {
    printf("it's not palindrome number\n");
}
return 0;
}
